    "  rock_playlists_dfs_list.append(temp_playlist_audio)\n",
    "    \n",
    "# Concatenating the Audio Features DataFrames\n",
    "rock_playlists_df = pd.concat(rock_playlists_dfs_list)\n",
    "rock_playlists_df"
